<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Employee</servlet-name>
        <servlet-class>Employee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewClient</servlet-name>
        <servlet-class>NewClient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Client</servlet-name>
        <servlet-class>Client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckInfo</servlet-name>
        <servlet-class>CheckInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deposit</servlet-name>
        <servlet-class>Deposit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Withdraw</servlet-name>
        <servlet-class>Withdraw</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Balance</servlet-name>
        <servlet-class>Balance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loan</servlet-name>
        <servlet-class>Loan</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Employee</servlet-name>
        <url-pattern>/Employee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewClient</servlet-name>
        <url-pattern>/NewClient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Client</servlet-name>
        <url-pattern>/Client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckInfo</servlet-name>
        <url-pattern>/CheckInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deposit</servlet-name>
        <url-pattern>/Deposit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Withdraw</servlet-name>
        <url-pattern>/Withdraw</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Balance</servlet-name>
        <url-pattern>/Balance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loan</servlet-name>
        <url-pattern>/Loan</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
